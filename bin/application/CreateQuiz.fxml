<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600" prefWidth="640" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CreateQuizController">
   <children>
      <Label layoutX="98.0" layoutY="25.0" text="Quiz ID" />
      <Label layoutX="98.0" layoutY="53.0" text="Question No" />
      <Text fx:id="quizID" layoutX="190.0" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      <Text fx:id="questionID" layoutX="190.0" layoutY="66.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      <TextField fx:id="question" layoutX="95.0" layoutY="170.0" prefHeight="25.0" prefWidth="444.0" promptText="Enter Question" />
      <TextField fx:id="option1" layoutX="95.0" layoutY="215.0" prefHeight="25.0" prefWidth="444.0" promptText="Option 1" />
      <TextField fx:id="option2" layoutX="95.0" layoutY="251.0" prefHeight="25.0" prefWidth="444.0" promptText="Option 2" />
      <TextField fx:id="option3" layoutX="95.0" layoutY="288.0" prefHeight="25.0" prefWidth="444.0" promptText="Option 3" />
      <TextField fx:id="option4" layoutX="95.0" layoutY="327.0" prefHeight="25.0" prefWidth="444.0" promptText="Option 4" />
      <Button fx:id="addQuestion" layoutX="398.0" layoutY="441.0" mnemonicParsing="false" onAction="#onCreateNextQuestion" text="Add Next Quesiton" />
      <TextField fx:id="answer" layoutX="95.0" layoutY="365.0" prefHeight="25.0" prefWidth="444.0" promptText="Answer Key" />
      <Button fx:id="submit" layoutX="235.0" layoutY="540.0" mnemonicParsing="false" onAction="#onBackToHome" prefHeight="24.0" prefWidth="111.0" text="Finish" />
   </children>
</AnchorPane>
